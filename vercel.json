{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/test-node",
      "dest": "/api/test-node.js"
    },
    {
      "src": "/api/scrape-simple",
      "dest": "/api/scrape-simple.js"
    },
    {
      "src": "/api/scrape",
      "dest": "/api/scrape.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
